        <section class="blog-contain">
            <% if (data && data.allBlogPosts && data.allBlogPosts.length > 0) { %>
                <% data.allBlogPosts.forEach(blogPost => { %>
                  <div class="blog-post">
                    <p class="publish-date-text"><%= blogPost.publishDate %></p>
                    <h4><%= blogPost.title %></h4>
                    <% if (blogPost.authors && blogPost.authors.length > 0) { %>
                      <ul>
                        <% blogPost.authors.forEach(author => { %>
                          <li>
                            <!-- <img src="<%= author.image.filename %>" alt="<%= author.name %>" width="<%= author.image.width %>" height="<%= author.image.height %>"> -->
                            <span>By <%= author.name %></span>
                          </li>
                        <% }); %>
                      </ul>
                    <% } %>
                  </div>
                <% }); %>
              <% } else { %>
                <p>No blog posts found.</p>
              <% } %>                       
        </section>
